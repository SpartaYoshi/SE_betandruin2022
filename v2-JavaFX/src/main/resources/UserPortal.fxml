         <font>
            <Font name="Trebuchet MS Bold" size="17.0" />
         </font>
      </Button>
      <Button layoutX="187.0" layoutY="240.0" mnemonicParsing="false" onAction="#selectRemoveABet" prefHeight="50.0" prefWidth="226.0" styleClass="btn-default" text="Remove a Bet">
