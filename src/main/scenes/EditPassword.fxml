      <Label fx:id="messageLabelPassword" layoutX="124.0" layoutY="332.0" prefHeight="18.0" prefWidth="366.0" text="  ">
         <font>
            <Font name="Segoe UI Bold" size="12.0" />
         </font></Label>
      <Label fx:id="usernameLabel" layoutX="100.0" layoutY="162.0" prefHeight="18.0" prefWidth="395.0">
         <font>
            <Font name="Segoe UI Bold" size="12.0" />
         </font></Label>
      <Button layoutX="42.0" layoutY="319.0" mnemonicParsing="false" onAction="#selectBack" prefHeight="25.0" prefWidth="62.0" styleClass="btn-danger" text="%Back">
         <font>
            <Font name="Segoe UI Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
