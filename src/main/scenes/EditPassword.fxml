            <Font name="Trebuchet MS Bold" size="35.0" />
         </font>
      </Label>
      <Button fx:id="editProfileButton" layoutX="224.0" layoutY="282.0" mnemonicParsing="false" onAction="#onEditProfileButton" prefHeight="32.0" prefWidth="145.0" styleClass="btn-info" text="%EditPassword">
         <font>
            <Font name="Trebuchet MS Bold" size="14.0" />
         </font>
      </Button>
      <ImageView fitHeight="55.0" fitWidth="68.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
