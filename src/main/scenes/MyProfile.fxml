<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="353.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uicontrollers.MyProfileController">
   <children>
      <Label fx:id="usernameLabel" layoutX="237.0" layoutY="98.0" text="Label">
         <font>
            <Font name="Trebuchet MS" size="14.0" />
         </font></Label>
      <Label fx:id="nameLabel" layoutX="237.0" layoutY="191.0" text="Label">
         <font>
            <Font name="Trebuchet MS" size="14.0" />
         </font></Label>
      <Label fx:id="surnameLabel" layoutX="237.0" layoutY="235.0" text="Label">
         <font>
            <Font name="Trebuchet MS" size="14.0" />
         </font></Label>
      <Label fx:id="birthDateLabel" layoutX="237.0" layoutY="277.0" text="Label">
         <font>
            <Font name="Trebuchet MS" size="14.0" />
         </font></Label>
      <ImageView fx:id="imageUser" fitHeight="150.0" fitWidth="200.0" layoutX="23.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/imageUser.jpg" />
         </image>
      </ImageView>
      <Button fx:id="onEditProfileButton" layoutX="29.0" layoutY="189.0" mnemonicParsing="false" onAction="#onEditProfileButton" prefHeight="26.0" prefWidth="150.0" styleClass="btn-primary" text="%EditPassword">
         <font>
            <Font name="Segoe UI Bold" size="12.0" />
         </font></Button>
      <Label fx:id="passwordLabel" layoutX="237.0" layoutY="144.0" text="Label">
         <font>
            <Font name="Trebuchet MS" size="14.0" />
         </font></Label>
      <Label alignment="CENTER" layoutX="198.0" layoutY="14.0" prefHeight="66.0" prefWidth="365.0" text="%MyProfile" textAlignment="CENTER" textOverrun="CLIP">
         <font>
            <Font name="Trebuchet MS Bold" size="35.0" />
         </font>
      </Label>
      <Button fx:id="myResultsButton" layoutX="29.0" layoutY="226.0" mnemonicParsing="false" onAction="#onMyResultsButton" prefHeight="26.0" prefWidth="150.0" styleClass="btn-warning" text="%MyResults">
         <font>
            <Font name="Segoe UI Bold" size="12.0" />
         </font></Button>
      <Button fx:id="myResultsButton1" layoutX="29.0" layoutY="263.0" mnemonicParsing="false" onAction="#onShowMovementsButton" prefHeight="26.0" prefWidth="150.0" styleClass="btn-warning" text="%ShowMovements">
         <font>
            <Font name="Segoe UI Bold" size="12.0" />
         </font></Button>
      <Button fx:id="backButton" layoutX="51.0" layoutY="302.0" mnemonicParsing="false" onAction="#selectBack" prefHeight="25.0" prefWidth="62.0" styleClass="btn-danger" text="%Back">
         <font>
            <Font name="Segoe UI Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
